<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">





<!-- bean definitions here -->
<bean class="com.cognizant.springCore.student" name="student1">
<property name="id">
<value>10</value>
</property>
<property name="name">
<value>Joseph</value>
</property>
<property name="rollNo">
<value>101</value>
</property>
</bean>

<bean class="com.cognizant.springCore.student" name="student2">
<property name="id">
<value>11</value>
</property>
<property name="name">
<value>Rinoy</value>
</property>
<property name="rollNo">
<value>102</value>
</property>
</bean>



</beans>